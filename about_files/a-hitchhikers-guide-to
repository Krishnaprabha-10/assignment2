			intervalCounter = 0;
			clearInterval(metadataCheckInterval);
			metadataCheckInterval = setInterval( function() {
				intervalCounter += 1;
				if (intervalCounter >= maxIntervalCounter) {
					clearInterval(metadataCheckInterval);
				} else if ( keepReadingList.length > 0 ) {
					clearInterval(metadataCheckInterval);
					keepReadingGAList(keepReadingList);
				} else if ( explainerList.length > 0 ) {
					clearInterval(metadataCheckInterval);
					explainerGAList(explainerList);
				}
			}, 500 );
		};
